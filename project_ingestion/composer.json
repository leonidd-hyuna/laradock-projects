{
    "name": "jomedia/project_ingestion",
    "version": "2.0.1",
    "description": "Feed content download & ingestion",
    "type": "console",

    "minimum-stability" : "dev",
    "prefer-stable": true,

    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "ext-zlib": "*",
        "ext-libxml": "*",
        "ext-pdo_mysql": "*",
        "ext-imagick": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-redis": "*",
        "silex/silex": "1.2.*",
        "phpseclib/phpseclib": "~2.0",
        "symfony/console": "v3.2.4",
        "doctrine/dbal": "v2.5.12",
        "facile-it/doctrine-mysql-come-back": "v1.5.1",
        "aws/aws-sdk-php-silex": "~2.0",
        "igorw/config-service-provider": "v1.2.2",
        "php-imap/php-imap": "~2.0",
        "ebichan/silex-amqp-provider": "dev-master",
        "fluent/logger": "v0.3.7",
        "php-curl-class/php-curl-class": "dev-master",
        "monolog/monolog": "~1.23",
        "swiftmailer/swiftmailer": "v5.4.0",
        "phpoffice/phpexcel": "1.8.1",
        "domnikl/statsd": "~2.1",
        "brightcove/api": "1.0",
        "cpliakas/jira": "~1.0",
        "zencoder/zencoder-php": "~2.2",
        "jbroadway/urlify": "1.1.0-stable",
        "phpunit/php-code-coverage": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "phpunit/dbunit": ">=1.2",
        "doctrine/migrations": "1.3.0",
        "squizlabs/php_codesniffer": "*",
        "brianium/paratest": "^0.14.0",
        "codeception/codeception": "2.3.*",
        "redmatter/codeception-multidb": "~2.0@dev"
    },
    "autoload": {
        "psr-0": {
            "JoMedia\\": "src",
            "Tools\\": "",
            "Tests\\": ""
        }
    }
}
